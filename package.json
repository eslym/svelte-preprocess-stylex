{
  "name": "@eslym/svelte-preprocess-stylex",
  "version": "0.1.5",
  "module": "index.ts",
  "type": "module",
  "description": "Svelte preprocessor to make stylex more readable in svelte.",
  "keywords": [
    "svelte",
    "preprocess",
    "stylex"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eslym/svelte-preprocess-stylex.git"
  },
  "license": "MIT",
  "author": {
    "name": "0nepeop1e",
    "email": "engshun.lim@gmail.com",
    "url": "https://0nepeop1e.com"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.5.0",
    "@types/bun": "^1.2.2",
    "@types/estree": "^1.0.6",
    "prettier": "^3.5.1",
    "prettier-plugin-svelte": "^3.3.3",
    "rollup": "^3.29.5",
    "rollup-plugin-dts": "^4.2.3",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "magic-string": ">=0.30.11",
    "svelte": ">=5.0.0"
  },
  "optionalDependencies": {
    "@stylexjs/stylex": "^0.11.1"
  }
}
